{
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ]
    },
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {},
    "editor.cursorStyle": "line",
    "editor.fontFamily": "SauceCodePro Nerd Font, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 17,
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": false,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.rename.enablePreview": false,
    "editor.renderControlCharacters": true,
    "editor.rulers": [
        88
    ],
    "editor.stickyScroll.enabled": true,
    "editor.stickyTabStops": true,
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "markdown",
    "files.exclude": {
        "**/.*_cache": true,
        "**/.direnv": true,
        "**/.env": true,
        "**/.venv": true,
        "**/.vscode": true
    },
    "files.trimFinalNewlines": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.timeline.showUncommitted": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": true
    },
    "gitlens.advanced.blame.delayAfterEdit": 2000,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.views.searchAndCompare.pullRequests.enabled": false,
    "json.schemas": [],
    "jupyter.allowUnauthorizedRemoteConnection": true,
    "jupyter.askForKernelRestart": false,
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "projectManager.any.ignoredFolders": [
        "node_modules",
        "out",
        "typings",
        "test",
        ".env",
        ".venv",
        ".pytest_cache",
        ".mypy_cache"
    ],
    "projectManager.git.baseFolders": [
        "/Users/tdhopper/c",
        "/Users/tdhopper/repos"
    ],
    "projectManager.vscode.ignoredFolders": [
        "node_modules",
        "out",
        "typings",
        "test",
        ".venv"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.envFile": "",
    "python.experiments.enabled": false,
    "python.experiments.optInto": [],
    "python.languageServer": "Pylance",
    "python.testing.pytestEnabled": true,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.altClickMovesCursor": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.scrollback": 50000,
    "trailing-spaces.deleteModifiedLinesOnly": true,
    "trailing-spaces.trimOnSave": true,
    "vim.easymotion": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<Up>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<Down>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<Left>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<Right>"
            ],
            "after": [
                "<Nop>"
            ]
        }
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<Esc>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.leader": " ",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                ":reg"
            ]
        },
        {
            "before": [
                "<Up>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<Down>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<Left>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<Right>"
            ],
            "after": [
                "<Nop>"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "d"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ],
            "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ],
            "when": "canNavigateBack"
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ],
            "when": "canNavigateForward"
        },
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        }
    ],
    "vim.smartRelativeLine": true,
    "vim.statusBarColors.surroundinputmode": "",
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ],
            "when": "editorTextFocus && !editorReadonly"
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d",
            ]
        }
    ],
    "workbench.colorTheme": "Codesso Light",
    "workbench.colorCustomizations": {
        "terminal.background": "#fff",
        "sideBar.background": "#fff"
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabActionLocation": "left",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editorAssociations": {
        "git-rebase-todo": "default",
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.startupEditor": "newUntitledFile",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.format.enable": false,
    "yaml.validate": false,
    "zenMode.centerLayout": false,
    "workbench.sideBar.location": "right",
    "window.titleBarStyle": "native",
    "apc.electron": {
        "titleBarStyle": "customButtonsOnHover",
    },
    "apc.header": {
        "height": 37
    },
    "apc.sidebar.titlebar": {
        "height": 37
    },
    "apc.activityBar": {
        "size": 77,
        "itemSize": 48,
        "itemMargin": 0
    },
    "apc.stylesheet": {
        ".custom-sidebar-titlebar .sidebar .composite.title": "padding-left: 0;", // Don't indent the sidebar title. We want it horizontally aligned to the left so that it makes a nice vertical column with the controls in the sidebar.
        ".custom-sidebar-titlebar .sidebar .composite.title .inline-titlebar-placeholder": "padding-left: 0 !important;", // Don't indent the sidebar title. Part 2 to remove the empty space to the left of the sidebar title and complete the proper horizontal left alignment.
        ".monaco-workbench .part.statusbar>.items-container>.statusbar-item.left.first-visible-item": "padding-left: 0;", // Don't indent the statusbar items. This horizontally aligns the statusbar items with the "Problems" header in the panel, which looks nice.
    },
    "editor.glyphMargin": false,
    "markdown.marp.breaks": "off",
    "markdown.marp.pdf.noteAnnotations": true,
    "workbench.activityBar.location": "hidden",
    "git.enableStatusBarSync": false,
    "gitlens.statusBar.enabled": false,
    "apc.statusBar": {},
    "gitlens.statusBar.pullRequests.enabled": false,
    "gitlens.graph.statusBar.enabled": false,
    "terminal.integrated.stickyScroll.enabled": true,
}